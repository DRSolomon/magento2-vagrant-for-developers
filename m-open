#!/usr/bin/env bash

set -e

cd "$(dirname "${BASH_SOURCE[0]}")" && vagrant_dir=$PWD

source "${vagrant_dir}/scripts/functions.sh"

magento2_url_with_port80="$(minikube service magento2 --url)"
#magento2_url_with_port80="$(minikube service magento2 --url | grep -oE 'http:[^:]+'):80"
open ${magento2_url_with_port80}
